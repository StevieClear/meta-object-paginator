{
  "version": 2,
  "outputDirectory": ".",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  }
}